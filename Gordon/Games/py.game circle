import pygame
pygame.init()
window = pygame.display.set_mode((500, 400))
while True:
    pygame.draw.circle(window, (255, 255, 0),
                               (200, 200), 20, 0)
    
    pygame.display.update()
    pygame.draw.circle(window,(255, 255, 0),
                              (300, 200), 20, 2)